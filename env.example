# WebScraper API Configuration
# Copy this file to .env and fill in your values

# ScrapeGraph AI Configuration (Optional - can be set via web interface)
SCRAPEGRAPH_API_KEY=your-openai-or-google-api-key
SCRAPEGRAPH_MODEL=gpt-4o
SCRAPEGRAPH_BASE_URL=http://localhost:11434
SCRAPEGRAPH_TEMPERATURE=0.0

# Database Configuration (Optional - can be set via web interface)
DB_HOST=localhost
DB_NAME=scraper_db
DB_TABLE=proxies
DB_USER=postgres
DB_PASSWORD=your-password

# Application Settings
LOG_LEVEL=INFO
DEBUG=false 